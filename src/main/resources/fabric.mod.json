{
  "schemaVersion": 1,
  "id": "fabricpvp",
  "version": "${version}",
  "name": "FabricPvP",
  "description": "FabricPvP is a small, server-side Fabric mod that lets players choose whether they want to participate in PvP",
  "authors": [
    "CodedSakura",
    "Trent2087"
  ],
  "contact": {
    "sources": "https://github.com/Trent2087/FabricPvP",
    "repo": "https://github.com/Trent2087/FabricPvP",
    "issues": "https://github.com/Trent2087/FabricPvP/issues"
  },
  "license": "ISC",
  "icon": "assets/FabricPvP/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "mod.trent2087.fabricpvp.FabricPvP"
    ],
    "cardinal-components-entity": [
      "mod.trent2087.fabricpvp.PlayerComponentInitializer"
    ]
  },
  "mixins": [
    "fabricpvp.mixins.json"
  ],
  "custom": {
    "cardinal-components": ["fabricpvp:pvp"]
  },
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "1.18.x"
  }
}
